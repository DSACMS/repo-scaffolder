      "license-contains-license": {
        "level": "error",
        "rule": {
          "type": "file-contents",
          "options": {
            "globsAll": ["{docs/,.github/,}LICENSE*"],
            "content": "license",
            "flags": "i",
            "file-name": "LICENSE",
            "file-content": "MIT License\n\nCopyright (c) [year] [fullname]\n\nPermission is hereby granted..."
