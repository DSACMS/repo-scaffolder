        }
      },
      "license-contains-license": {
        "level": "error",
        "rule": {
          "type": "file-contents",
          "options": {
            "globsAll": [
                "{docs/,.github/,}LICENSE*"
            ],
            "content": "license",
            "flags": "i"
          }
        }
      },
      "security-contains-security-and-responsible-disclosure-policy": {
        "level": "error",
        "rule": {
          "type": "file-contents",
          "options": {
            "globsAll": [
              "{docs/,.github/,}SECURITY.md"
