{
  "extends": "https://raw.githubusercontent.com/DSACMS/repo-scaffolder/main/tier3/%7B%7Bcookiecutter.project_slug%7D%7D/repolinter.json",
  "$schema": "https://raw.githubusercontent.com/todogroup/repolinter/master/rulesets/schema.json",
  "version": 2,
  "axioms": {
    "linguist": "language",
    "licensee": "license",
    "packagers": "packager"
  },
  "rules": {
    "readme-contains-glossary": {
      "level": "error"
    },
    "contributing-contains-shipping-releases": {
      "level": "error"
    },
    "community-contains-alumni": {
      "level": "error"
    },
    "governance-file-exists": {
      "level": "error"
    },
    "governance-contains-governance": {
      "level": "error"
    },
    "readme-contains-agency-mission": {
      "level": "off"
    },
    "readme-contains-team-mission": {
      "level": "off"
    }
  }
}